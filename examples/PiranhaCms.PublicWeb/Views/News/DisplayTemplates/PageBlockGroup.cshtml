@model PiranhaCMS.ContentTypes.Blocks.PageBlockGroup
@using PiranhaCMS.Common.Extensions

<!-- Painéis temáticos -->
<section id="paineisCallout">
    <div class="wrap">
        <div class="grid-x small-up-1 medium-up-2 large-up-4 align-center">
            @if (Model.Items != null && Model.Items.Count > 0)
            {
                foreach (var item in Model.Items)
                {
                    <div class="column column-block">
                  
                            <partial name="PageBlock.cshtml" model="@item" />

                    </div>    
                }
             
            }
            
        </div>
    </div>
</section>
